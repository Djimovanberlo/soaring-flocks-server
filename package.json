{
  "name": "server-template",
  "version": "1.0.0",
  "description": "Simple express server with sequelize, pg and jwts",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "env DELAY=1000 npx nodemon index.js",
    "initdev": "npm run migrate && npm run seed",
    "migrate": "npx sequelize-cli db:migrate",
    "seed": "npx sequelize-cli db:seed:all",
    "resetDB": "npx sequelize-cli db:migrate:undo:all && npm run initdev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "template",
    "express",
    "jwt",
    "sequelize"
  ],
  "author": "Rein Op 't Land",
  "license": "MIT",
  "dependencies": {
    "apollo-server": "^2.15.0",
    "bcrypt": "^5.0.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "graphql": "^15.1.0",
    "graphql-subscriptions": "^1.1.0",
    "graphql-tag": "^2.10.3",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.9.1",
    "node-cron": "^2.0.3",
    "pg": "^7.18.2",
    "sequelize": "^5.21.5",
    "sequelize-cli": "^5.5.1"
  },
  "devDependencies": {
    "axios": "^0.19.2",
    "nodemon": "^2.0.2"
  }
}
